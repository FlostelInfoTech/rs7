{
  "name": "rs7-wasm",
  "version": "0.4.0",
  "description": "WebAssembly bindings for RS7 - HL7 v2.x library for JavaScript/TypeScript",
  "main": "pkg/rs7_wasm.js",
  "types": "pkg/rs7_wasm.d.ts",
  "files": [
    "pkg"
  ],
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg",
    "build:node": "wasm-pack build --target nodejs --out-dir pkg-node",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg-bundler",
    "build:all": "npm run build && npm run build:node && npm run build:bundler",
    "test": "wasm-pack test --headless --firefox",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.flostel.com/alexshao/rs7"
  },
  "keywords": [
    "hl7",
    "healthcare",
    "medical",
    "wasm",
    "webassembly",
    "parser",
    "health-information-exchange",
    "interoperability"
  ],
  "author": "Alex Shao <alex.fanshao@gmail.com>",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "wasm-pack": "^0.12.0"
  }
}
